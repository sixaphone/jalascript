{
  "name": "@sixaphone/jalascript",
  "version": "1.0.0",
  "description": "Minimalistic express server framework with typescript integration.",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack",
    "start": "node dist/app.js",
    "watch": "nodemon src/app.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "types": "@types/index.d.ts",
  "keywords": [
    "express",
    "typescript",
    "web",
    "framework"
  ],
  "author": {
    "email": "miki.halilcevic@gmail.com",
    "name": "sixaphone",
    "url": "https://github.com/sixaphone"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "mongoose": "^5.10.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.1",
    "@types/nodemon": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^7.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "save-dev": "0.0.1-security",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
